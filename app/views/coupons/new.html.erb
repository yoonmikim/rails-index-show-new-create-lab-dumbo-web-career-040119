<h1>New Coupon Form</h1>

<!--<%= render "form" %>-->

<%= form_for @coupon do |form| %>

  <%= form.label :coupon_code %> 
  <%= form.text_area :coupon_code %>
  
  <%= form.label :store %> 
  <%= form.text_field :store %>
  
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  
  <%= form.submit %>
<% end %>